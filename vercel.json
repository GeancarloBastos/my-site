{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/api/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_BASE_URL": "https://api-meu-site.vercel.app"
  }
}